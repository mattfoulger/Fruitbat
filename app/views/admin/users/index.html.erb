<h1>Users List</h1>
<hr>

<table class="table">
<tr>
  <th>ID</th>
  <th>First name</th>
  <th>Last name</th>
  <th>Admin?</th>
  <th></th>
</tr>
  
<% @users.each do |user| %>
  <tr>
    <td><%= user.id %></td>
    <td><%= user.firstname %></td>
    <td><%= user.lastname %></td>
    <td><%= user.admin %></td>
    <td><%= link_to "Edit", edit_admin_user_path(user) %></td>
  </tr>
<% end %>
</table>